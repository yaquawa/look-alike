{
    "name": "yaquawa/look-alike",
    "description": "A tiny lib for copy properties from one to another.",
    "keywords": [
        "dev",
        "debug",
        "laravel"
    ],
    "license": "MIT",
    "type": "project",
    "scripts": {
        "fix-cs": ["prettier 'src/**/*.php' --write"]
    },
    "require": {
        "php": "^7.2"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.3"
    },
    "autoload": {
        "psr-4": {
            "Yaquawa\\LookAlike\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
